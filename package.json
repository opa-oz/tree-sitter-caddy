{
  "name": "tree-sitter-caddy",
  "version": "0.1.1",
  "description": "Caddyfile support",
  "repository": "https://github.com/opa-oz/tree-sitter-caddy",
  "license": "GPL-3.0",
  "author": {
    "name": "Vladimir <opa-oz> Levin",
    "email": "opaozhub@gmail.com",
    "url": "https://github.com/opa-oz"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "caddy"
  ],
  "files": [
